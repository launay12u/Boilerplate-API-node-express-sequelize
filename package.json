{
    "name": "api-rest-nodejs-cockpit",
    "version": "0.0.1",
    "description": "A Boilerplate application for building APIs with nodeJS",
    "homepage": "https://github.com/launay12u/Boilerplate-API-node-express-sequelize#readme",
    "repository": "github:launay12u/Boilerplate-API-node-express-sequelize",
    "bugs": {"url":"https://github.com/launay12u/Boilerplate-API-node-express-sequelize/issues"},
    "contributors": [
        {
            "name": "Guillaume Launay",
            "url": "https://github.com/launay12u"
        },
        {
            "name": "Anthony Olla",
            "url": "https://github.com/ollaay"
        },
        {
            "name": "Romain Postel",
            "url": "https://github.com/postel5u"
        }
    ],
    "main": "index.js",
    "private": false,
    "engines": {
        "node": ">=8.10.0",
        "npm": ">=5.6.0",
        "yarn": ">=1.5.1"
    },
    "license": "GPL-3.0",
    "scripts": {
        "start": "gulp serve",
        "start:debug": "cross-env DEBUG=amida-api-boilerplate:* yarn start",
        "build": "gulp",
        "copy": "gulp copy",
        "babel": "gulp babel",
        "lint": "esw *.js server config --color",
        "lint:watch": "yarn lint -- --watch",
        "beautify": "es-beautifier -u -c .eslintrc.js *.js server config",
        "precommit": "yarn lint && yarn test",
        "pretest":"cross-env NODE_ENV=test gulp initTestDatabase",
        "test": "cross-env NODE_ENV=test jest server/tests",
        "test:watch": "jest --watch",
        "test:coverage": "cross-env NODE_ENV=test jest server/tests --coverage",
        "test:check-coverage": "yarn test:coverage && nyc check-coverage",
        "report-coverage": "coveralls < ./coverage/lcov.info"
    },
    "keywords": [
        "express",
        "node",
        "node.js",
        "es6",
        "jest",
        "nyc",
        "REST",
        "API",
        "boilerplate"
    ],
    "jest": {
        "verbose": false,
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/coverage/",
            "/dist/"
        ],
        "testEnvironment": "node"
    },
    "dependencies": {
        "async": "^2.6.1",
        "babel-register": "6.26.0",
        "bluebird": "3.4.6",
        "body-parser": "1.15.2",
        "compression": "1.6.2",
        "cookie-parser": "1.4.3",
        "cors": "2.8.1",
        "debug": "^2.4.5",
        "del": "^2.2.0",
        "dotenv": "^4.0.0",
        "express": "4.14.0",
        "express-jwt": "5.1.0",
        "express-validation": "1.0.1",
        "express-winston": "2.1.2",
        "gulp": "3.9.1",
        "gulp-load-plugins": "^1.2.0",
        "helmet": "3.1.0",
        "http-status": "^0.2.0",
        "joi": "10.0.6",
        "jsonwebtoken": "7.1.9",
        "lodash": "4.17.10",
        "method-override": "^2.3.5",
        "morgan": "1.7.0",
        "mysql2": "1.6.4",
        "run-sequence": "^1.1.5",
        "sequelize": "4.38.0",
        "supertest": "2.0.1",
        "supertest-as-promised": "4.0.2",
        "winston": "2.3.0"
    },
    "devDependencies": {
        "@babel/core": "false7.2.2",
        "babel-cli": "6.18.0",
        "babel-core": "6.18.0",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-plugin-istanbul": "false5.1.0",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-preset-es2015": "6.16.0",
        "babel-preset-stage-2": "6.18.0",
        "commitizen": "^2.9.2",
        "coveralls": "^2.11.6",
        "cross-env": "3.1.3",
        "eslint": "3.16.1",
        "eslint-config-airbnb-base": "7.1.0",
        "eslint-plugin-import": "1.16.0",
        "eslint-watch": "2.1.14",
        "gulp-babel": "6.1.2",
        "gulp-newer": "^1.1.0",
        "gulp-nodemon": "^2.0.6",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-util": "^3.0.7",
        "husky": "^0.13.1",
        "jest": "23.4.2",
        "jsdom": "11.12.0",
        "nyc": "false13.1.0",
        "validate-commit-msg": "^2.6.1"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-2"
        ],
        "plugins": [
            "add-module-exports",
            "transform-object-rest-spread"
        ],
        "ignore": [
            "build/yarn/**/*.js",
            "build/nodejs/**/*.js"
        ]
    }
}
